{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "frs/dscr-calculator",
	"version": "1.1.0",
	"title": "DSCR Calculator",
	"category": "frs-calculators",
	"icon": "calculator",
	"description": "Calculate Debt Service Coverage Ratio for investment property loans.",
	"keywords": ["dscr", "investment", "calculator", "rental", "property"],
	"textdomain": "frs-mortgage-calculator",
	"attributes": {
		"userId": {
			"type": "number",
			"default": 0
		},
		"showLeadForm": {
			"type": "boolean",
			"default": true
		},
		"gradientStart": {
			"type": "string",
			"default": "#2563eb"
		},
		"gradientEnd": {
			"type": "string",
			"default": "#2dd4da"
		},
		"webhookUrl": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"render": "file:./render.php"
}
